                    <CommandText>SELECT        SamDailyCutStatus.SamCutttingDailyId, SamDailyCutStatus.SampCutreqID, SampCutReqMaster.ReqNum, PatterRefMaster.PatterRefNum, SampleType.SampleType, PatternStyle.StyleName, 
                         BuyerMaster.BuyerName, SampCutReqMaster.Fabric, SamCutAssignmentMaster.PatternCompletedDate, SamDailyCutStatus.DateofAction, SampCutReqMaster.SampleRequiredDate, SampCutReqMaster.Qty, 
                         SampCutReqMaster.Size1CutQty + SampCutReqMaster.Size2CutQty + SampCutReqMaster.Size3CutQty + SampCutReqMaster.Size4CutQty + SampCutReqMaster.Size5CutQty + SampCutReqMaster.Size6CutQty AS
