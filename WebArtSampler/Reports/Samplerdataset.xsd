                          SampCutReqMaster.AddedBy
FROM            SamDailyCutStatus INNER JOIN
                         SampCutReqMaster ON SamDailyCutStatus.SampCutreqID = SampCutReqMaster.SampCutreqID INNER JOIN
                         PatterRefMaster ON SampCutReqMaster.PatternRefID = PatterRefMaster.PatternRefID INNER JOIN
                         PatternStyle ON SampCutReqMaster.PatternStyleID = PatternStyle.PatternStyleID INNER JOIN
